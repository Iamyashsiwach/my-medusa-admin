{
  "redirects": [
    {
      "source": "/",
      "destination": "/app",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/app/assets/:path*",
      "destination": "/assets/:path*"
    },
    {
      "source": "/app/:path*",
      "destination": "/index.html"
    }
  ]
} 